{
  "name": "app2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve":"npm run serve:client && npm run serve:server",
    "serve:client": "npx serve dist/client -p 3001",
    "serve:server": "npx serve dist/server -p 3001",
    "build": "npm run build:client && npm run build:server",
    "build:client": "rimraf dist/client && webpack --mode production --config config/webpack.client.js",
    "build:server": "rimraf dist/server && webpack --mode production --config config/webpack.server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@module-federation/node": "^2.0.4",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.6.0",
    "rimraf": "^5.0.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  }
